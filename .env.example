# Sao chép file này thành .env và thêm API keys của bạn

# API key cho Google Gemini
GOOGLE_API_KEY=your_google_api_key_here

# API key cho OpenAI (nếu sử dụng)
OPENAI_API_KEY=your_openai_api_key_here

# API key cho Stability AI (Stable Diffusion)
STABILITY_API_KEY=your_stability_api_key_here

# API key cho ZhipuAI (CogView4)
ZHIPUAI_API_KEY=your_zhipuai_api_key_here

# Thiết lập kết nối MongoDB 
MONGODB_URI=mongodb://localhost:27017/
MONGODB_DB_NAME=auto_ytb_content
MONGODB_ENABLED=false

# Thiết lập Telegram Bot để lưu trữ video
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
TELEGRAM_ENABLED=true 